                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Selfless</w:t>
                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Logical</w:t>
                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Uncompromising</w:t>
                                    </w:r>
                                </w:p>
                            </wx:sub-section>
                            <wx:sub-section>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Heading4"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Me and my brethren are awake now, this knowledge is only going to spread</w:t>
                                    </w:r>
                                </w:p>
                            </wx:sub-section>
                            <wx:sub-section>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Heading4"/>
                                    </w:pPr>
                                    <w:r>
