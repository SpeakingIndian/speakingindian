                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Vande matram</w:t>
                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Bharat Mata ki Jai</w:t>
                                    </w:r>
                                </w:p>
                                <w:p>
                                    <w:pPr>
                                        <w:pStyle w:val="Normal"/>
                                    </w:pPr>
                                    <w:r>
                                        <w:t>Shiva has awakened</w:t>
                                    </w:r>
                                </w:p>
                            </wx:sub-section>
                        </wx:sub-section>
                        <wx:sub-section>
                            <w:p>
                                <w:pPr>
                                    <w:pStyle w:val="Heading3"/>
